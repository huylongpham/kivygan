Mainscreenmanager:
    id: screen_manager
    Screen:
        name: 'main'
        canvas:
            Color:
                rgb: 44/255, 53/255, 99/255
            Rectangle:
                pos: self.pos
                size: self.size

        BoxLayout:
            orientation: 'vertical'
            Label:
                text: 'Date'
                id: ngay
                pos_hint: {'top':1}
                size_hint: 1, 0.1
            BoxLayout:
                BoxLayout:
                    orientation: 'vertical'
                    size_hint: 0.1, 1
                    Button:
                        text: 'DB'
                    Button:
                        text: 'G1'
                    Button:
                        text: 'G2'
                    Button:
                        text: 'G3'
                    Button:
                        text: 'G3'
                    Button:
                        text: 'G4'
                    Button:
                        text: 'G5'
                    Button:
                        text: 'G5'
                    Button:
                        text: 'G6'
                    Button:
                        text: 'G7'
                BoxLayout:
                    orientation: 'vertical'
                    text: 'result'
                    BoxLayout:
                        Button:
                            text: 'DB'
                            id: gdb
                            on_press: screen_manager.crawler()
                    BoxLayout:
                        Button:
                            text: 'G1'
                            id: g1
                    BoxLayout:
                        Button:
                            text: 'G2'
                            id: g21
                        Button:
                            text: 'G2'
                            id: g22
                    BoxLayout:
                        Button:
                            text: 'G3'
                            id: g31
                        Button:
                            text: 'G3'
                            id: g32
                        Button:
                            text: 'G3'
                            id: g33
                    BoxLayout:
                        Button:
                            text: 'G3'
                            id: g34
                        Button:
                            text: 'G3'
                            id: g35
                        Button:
                            text: 'G3'
                            id: g36
                    BoxLayout:
                        Button:
                            text: 'G4'
                            id: g41
                        Button:
                            text: 'G4'
                            id: g42
                        Button:
                            text: 'G4'
                            id: g43
                        Button:
                            text: 'G4'
                            id: g44
                    BoxLayout:
                        Button:
                            text: 'G5'
                            id: g51
                        Button:
                            text: 'G5'
                            id: g52
                        Button:
                            text: 'G5'
                            id: g53
                    BoxLayout:
                        Button:
                            text: 'G5'
                            id: g54
                        Button:
                            text: 'G5'
                            id: g55
                        Button:
                            text: 'G5'
                            id: g56
                    BoxLayout:
                        Button:
                            text: 'G6'
                            id: g61
                        Button:
                            text: 'G6'
                            id: g62
                        Button:
                            text: 'G6'
                            id: g63
                    BoxLayout:
                        Button:
                            text: 'G7'
                            id: g71
                        Button:
                            text: 'G7'
                            id: g72
                        Button:
                            text: 'G7'
                            id: g73
                        Button:
                            text: 'G7'
                            id: g74
            BoxLayout:
                size_hint: 1, 0.1
                Button:
                    text: 'show'
                    on_press: screen_manager.print_forward()
                Button:
                    text: 'back'
                    on_press: screen_manager.print_backward()
                Button:
                    text: 'play'
                    on_release: screen_manager.current = 'second'

    Screen:
        name: 'second'
        GridLayout:
            cols: 2
            Button:
                text: 'gan'
                on_press: screen_manager.current = 'gan'
            Button:
                text: 'max'
                on_press: screen_manager.current = 'max'
            Button:
