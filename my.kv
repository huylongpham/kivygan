Screenmanager:
    Mainscreen:
    Ganscreen:
    Twinscreen:
    Maxganscreen:
    Foursetscreen:
    Topscreen:
    Topupscreen:
    Topdownscreen:

<P>:
    Label:
        text: "popup"
        size_hint: 0.2, 0.2
        pos_hint: 0.5, 0.5

<Mainscreen>:
    name: 'main'
    BoxLayout:
        orientation: 'vertical'
        Label:
            text: "chon kieu choi"
        Button:
            text: 'gan huong'
            on_press: root.manager.current = 'gan'
        Button:
            text: 'twin gan huong'
            on_press: root.manager.current = 'twin'
        Button:
            text: 'maxgan huong'
            on_press: root.manager.current = 'maxgan'
        Button:
            text: 'fourset'
            on_press: root.manager.current = 'fourset'
        Button:
            text: 'top'
            on_press: root.manager.current = 'top'
        Button:
            text: 'topup'
            on_press: root.manager.current = 'topup'
        Button:
            text: 'topdown'
            on_press: root.manager.current = 'topdown'


        Button:
            text: 'get result'
            on_release:

                root.getresult()


<Ganscreen>:
    name: 'gan'
    ngay: ngay
    number: number
    ketqua: ketqua
    gan: gan
    huong: huong
    xeo: xeo
    kqxeo: kqxeo

    BoxLayout:
        orientation: 'vertical'
        size_hint: (1, 0.95)
        pos_hint: {'top':1}
        Label:

            text: "gan "
            id: ngay

        Label:
            text: "number"

        TextInput:
            text: "number"
            id: number
            multiline: True
            halign: 'left'

        Label:
            text: "ketqua"

        TextInput:
            text: "ketqua"
            id: ketqua
            multiline: True
            halign: 'left'

        Label:
            text: "gan"

        TextInput:
            text: "gan"
            id: gan
            multiline: True
            halign: 'left'

        Label:
            text: "huong"

        TextInput:
            text: "huong"
            id: huong
            multiline: True
            halign: 'left'

        Label:
            text: "xeo"

        TextInput:
            text: "xeo"
            id: xeo
            multiline: True
            halign: 'left'

        Label:
            text: "ketqua xeo"

        TextInput:
            text: "ketqua xeo"
            id: kqxeo
            multiline: True
            halign: 'left'
    BoxLayout:
        size_hint: (1, 0.05)
        Button:
            text: 'backmain'
            on_press: root.manager.current = 'main'
        Button:
            text: 'homsau'
            on_press: root.nextbut()
        Button:
            text: 'homtruoc'
            on_press: root.backbut()


